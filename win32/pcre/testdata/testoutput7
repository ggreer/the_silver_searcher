        Bra
        prop Any +
        prop Any
        prop Any +
        notprop Any
        prop Any +
        prop L&
        prop Any +
        prop L
        prop Any +
        prop Lu
        prop Any +
        prop Han
        prop Any +
        prop Xan
        prop Any +
        prop Xsp
        prop Any +
        prop Xps
        prop Xwd +
        prop Any
        prop Any +
        prop Xuc
        Ket
        End
------------------------------------------------------------------

